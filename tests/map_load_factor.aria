import Map from aria.structures.map;

func main() {
    val m = Map.new_with_capacity(5);

    m[0] = 1;
    m[1] = 2;
    m[2] = 3;
    m[3] = 4;
    m[4] = 5;
    m[5] = 6;
    m[6] = 7;
    m[7] = 8;
    m[8] = 9;
    m[9] = 10;
    m[10] = 11;
    m[11] = 12;
    m[12] = 13;
    m[13] = 14;
    m[14] = 15;
    m[15] = 16;
    m[16] = 17;
    m[17] = 18;
    m[18] = 19;
    m[19] = 20;

    assert m.len() == 20;
    for item in m {
        assert item.value == item.key + 1;
    }
}
