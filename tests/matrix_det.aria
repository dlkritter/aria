import Matrix from aria.numerics.matrix;

func main() {
    val m = Matrix.new(3,3);

    m[0,0] = 3;
    m[0,1] = 3;
    m[0,2] = 3;
    m[1,0] = 4;
    m[1,1] = 2;
    m[1,2] = 5;
    m[2,0] = 1;
    m[2,1] = 6;
    m[2,2] = 2;

    assert m.determinant() == -21;

    m = Matrix.new(10,10);
    assert m.determinant() == 0;

    m[0,0] = 5;
    m[1,1] = 5;
    m[2,2] = 5;
    m[3,3] = 5;
    m[4,4] = 5;
    m[5,5] = 5;
    m[6,6] = 5;
    m[7,7] = 5;
    m[8,8] = 5;
    m[9,9] = 5;

    assert m.determinant() == 9765625;
}
