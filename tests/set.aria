import Set from aria.structures.set;

func main() {
    val s = Set.new();
    
    assert s.len() == 0;
    s.set(123);
    
    assert s.len() == 1;
    assert s.contains(123);
    assert !s.contains("123");

    s.set("hello");
    assert s.len() == 2;
    assert s.contains("hello");
    assert s.contains(123);

    s.remove(321);
    assert s.len() == 2;
    assert s.contains("hello");
    assert s.contains(123);

    s.remove(123);
    assert s.len() == 1;
    assert s.contains("hello");
    assert !s.contains(123);

    s = Set.new_with_items(1, 2, 3, 4, 5);
    assert s.len() == 5;
    assert s.contains(3);
    val count = 0;
    for x in s {
        count = count + x;
    }
    assert count == 15;
}
